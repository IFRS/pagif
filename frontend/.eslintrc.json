{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "@nuxt/eslint-config",
    "plugin:vuetify/base"
  ],
  "rules": {
    "vue/no-v-html": "off",
    "vue/multi-word-component-names": "off"
  },
  "globals": {
    "$fetch": "readonly",
    "globalThis": "readonly",
    "defineAppConfig": "readonly",
    "defineStore": "readonly",
    "acceptHMRUpdate": "readonly"
  }
}
