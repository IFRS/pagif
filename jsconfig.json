{
  "vueCompilerOptions": {
    "experimentalDisableTemplateSupport": true,
  },
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"],
      "@/*": ["./*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", ".nuxt", "dist"],
}
