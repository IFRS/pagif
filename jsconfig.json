{
  "vueCompilerOptions": {
    "target": 3
  },
  "compilerOptions": {
    "moduleResolution": "node",
    "module": "ES6",
    "target": "ES6",
    "baseUrl": ".",
    "paths": {
      "~/*": ["./frontend/*"],
      "@/*": ["./frontend/*"],
      "~~/*": ["./*"],
      "@@/*": ["./*"]
    }
  },
  "include": ["**/*"],
  "exclude": ["node_modules", ".nuxt", "dist", "public"]
}
